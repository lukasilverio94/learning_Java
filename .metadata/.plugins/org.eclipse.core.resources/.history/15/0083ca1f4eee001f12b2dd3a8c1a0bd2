package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("How many rooms will be rented?");
		int n = sc.nextInt();
		
		int[] vect = new int[n];
		
		System.out.println("Rent #" + (vect.length + 1));
		
		sc.close();
	}

}
