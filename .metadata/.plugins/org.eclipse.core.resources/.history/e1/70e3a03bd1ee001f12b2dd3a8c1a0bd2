package lists;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		
		list.add("Maria");
		list.add("Carol");
		list.add("Alex");
		list.add("Bob");
		
		for(String name: list) {
			System.out.println(name);
		}
	}

}
