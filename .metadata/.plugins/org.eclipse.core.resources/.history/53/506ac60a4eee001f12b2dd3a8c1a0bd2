package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("How many rooms will be rented?");
		int n = sc.nextInt();
		
		System.out.println("Rent #" + n + 1);
		
		sc.close();
	}

}
